<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:vm="using:Drizzle.Editor.ViewModels"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:ed="clr-namespace:Drizzle.Editor"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="Drizzle.Editor.Views.MainWindow"
        Icon="/Assets/avalonia-logo.ico"
        Title="Drizzle.Editor" CanResize="False" Width="1024" Height="768">

    <Design.DataContext>
        <vm:MainWindowViewModel/>
    </Design.DataContext>
    
    <Panel>
        <ContentControl Content="{Binding LingoVM.Frame}" />
    </Panel>
    
</Window>
